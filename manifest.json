{

  "manifest_version": 2,
  "name": "Personio Time Filler",
  "version": "1.0",

  "description": "Automatically fill in personio time tablesg.",
  "browser_action": {
    "name": "Click to get URL"
  },
  "background":{
        "scripts":["background.js"]
  },
  "permissions":["https://sharenow.personio.de/*"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+I"
      },
      "description": "Send a '_execute_browser_action' event"
    }
  }
}

